<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            name="%editor.name"
            icon="icons/CMakeLogo.gif"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            class="com.cthing.cmakeed.ui.editor.CMakeEditor"
            id="com.cthing.cmakeed.ui.editors.CMakeEditor">
         <contentTypeBinding contentTypeId="com.cthing.cmakeed.CMakeFileType"/>
      </editor>
   </extension>
   <extension
         point="org.eclipse.core.runtime.contentTypes">
      <content-type
            base-type="org.eclipse.core.runtime.text"
            file-extensions="cmake"
            file-names="CMakeLists.txt"
            id="com.cthing.cmakeed.CMakeFileType"
            name="%content-type.name"
            priority="normal"/>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="com.cthing.cmakeed.ui.prefs.PrefPage"
            id="com.cthing.cmakeed.ui.prefs.PreferencePage"
            name="%page.name"/>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer class="com.cthing.cmakeed.ui.prefs.Initializer"/>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc file="toc.xml" primary="true"/>
   </extension>

</plugin>
